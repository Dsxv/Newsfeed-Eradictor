{
  "short_name": "nfErad",
  "name": "NewsFeed Eradicator",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "permissions": [],
  "content_scripts" : [
    {
      "matches": ["https://www.facebook.com/*"],

      "js": ["content.js"],
      "css":["content.css"]

    }] ,
    "icons" : {
      "48" : "sed.png"
    }
}
